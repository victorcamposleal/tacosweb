{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*).html",    
        "dest": "/public/$1.html"
      },
      {
        "src": "/(.*)",         
        "dest": "index.js"
      }
    ]
  }
  